{
  "name": "MercuryWallet",
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src",
      "client-wasm/pkg"
    ],
    "modulePaths": [
      "src",
      "node_modules",
      "client-wasm/pkg"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "d.ts",
      "wasm"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "repository": "github:commerceblock/mercury-wallet",
  "version": "v0.2.2",
  "license": "MIT/Apache-2.0",
  "homepage": "./",
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "dev": "BROWSER=none nf start -p 3000",
    "postinstall": "electron-builder install-app-deps",
    "build-wasm": "wasm-pack build --dev client-wasm",
    "app-windows": "yarn install && yarn build && electron-builder -w -p always",
    "app-linux": "yarn install && yarn build && electron-builder -l AppImage -p always",
    "app-macos": "yarn install && yarn build && electron-builder -m -p always",
    "app-macos-local-test": "yarn install && yarn build && DEBUG=electron-builder electron-builder -m -p never",
    "app-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "devDependencies": {
    "@types/uuid": "^8.3.0",
    "concurrently": "^5.3.0",
    "electron": "^10.1.4",
    "electron-build-env": "^0.2.0",
    "electron-builder": "^22.9.1",
    "electron-builder-notarize": "^1.2.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-notarize": "^1.0.0",
    "foreman": "^3.0.1",
    "react-app-rewired": "^2.1.6",
    "react-scripts": "^3.4.3",
    "typescript": "^4.0.3",
    "wait-on": "^5.2.0",
    "wasm-loader": "^1.3.0"
  },
  "dependencies": {
    "@keep-network/electrum-client-js": "keep-network/electrum-client-js",
    "@reduxjs/toolkit": "^1.4.0",
    "@types/node": "^14.14.13",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "axios": "^0.21.0",
    "bech32": "^1.1.4",
    "bigint-mod-arith": "^2.0.8",
    "bip32": "^2.0.6",
    "bip32-utils": "^0.11.1",
    "bip39": "^3.0.3",
    "bitcoinjs-lib": "^5.2.0",
    "bn": "^1.0.5",
    "bootstrap": "^4.5.3",
    "buffer-json": "^2.0.0",
    "client-wasm": "file:client-wasm/pkg",
    "eciesjs": "^0.3.9",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.3.1",
    "electron-store": "^7.0.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-react-parser": "^0.14.2",
    "install": "^0.13.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "msgpack-lite": "^0.1.26",
    "net": "^1.0.2",
    "qrcode.react": "^1.0.1",
    "react": "^16.1.1",
    "react-bootstrap": "^1.4.0",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^16.1.1",
    "react-hook-form": "^6.15.1",
    "react-moment": "^1.0.0",
    "react-quantity-picker": "^1.0.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.6.2",
    "react-use-clipboard": "1.0.7",
    "superagent": "^6.1.0",
    "ts-jest": "^26.4.4",
    "typeforce": "^1.11.3",
    "uuid": "^8.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": {
    "name": "Mercury Wallet",
    "email": "main@mercurywallet.com",
    "url": "https://mercurywallet.com"
  },
  "build": {
    "appId": "com.mercurywallet.wallet",
    "productName": "mercurywallet",
    "copyright": "Copyright Â© 2020 ${author}",
    "afterSign": "electron-builder-notarize",
    "mac": {
      "category": "public.app-category.utilities",
      "hardenedRuntime": true,
      "entitlements": "./node_modules/electron-builder-notarize/entitlements.mac.inherit.plist",
      "icon": "./public/icons/mercury-symbol-tri-color.icns"
    },
    "linux": {
      "icon": "./public/icons/mercury-symbol-tri-color.icns"
    },
    "win": {
      "icon": "./public/icons/mercury-symbol-tri-color.png"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "commerceblock",
        "repo": "mercury-wallet"
      }
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1"
  }
}
